@SHOW_INFO
PRINT_ABL TARGET
PRINT_TALENT TARGET
PRINT_MARK TARGET
PRINT_EXP TARGET
WAIT

@SHOW_STATUS
DRAWLINE
PRINTV DAY+1
PRINT 日目
IF TIME == 0
	PRINTL  午前
ELSE
	PRINTL  午後
ENDIF
SIF ASSIPLAY
	PRINTFORM 助手：%NAME:ASSI%が
PRINTS NAME:TARGET
PRINTL 調教中
CALL LIFE_BAR

PRINT 気力
A = BASE:1
SIF BASE:1 < 0
	A = 0
BAR A,MAXBASE:1,32
PRINTFORML ({A}/{MAXBASE:1})
PRINT_PALAM TARGET

SIF MAXBASE:2 == 0
	MAXBASE:2 == 10000
SIF MAXBASE:MASTER:2 == 0
	MAXBASE:MASTER:2 == 10000
IF ASSI >= 0
	SIF MAXBASE:ASSI:2 == 0
		MAXBASE:ASSI:2 == 10000
ENDIF

PRINT 射精
BAR BASE:MASTER:2,MAXBASE:MASTER:2,32
PRINTFORML ({BASE:MASTER:2}/{MAXBASE:MASTER:2})

IF ASSIPLAY
	IF TALENT:ASSI:121 || TALENT:ASSI:122
		PRINT 射精（助手）
		BAR BASE:ASSI:2,MAXBASE:ASSI:2,32
		PRINTFORML ({BASE:ASSI:2}/{MAXBASE:ASSI:2})
	ENDIF
ENDIF

IF TALENT:TARGET:121 || TALENT:TARGET:122
	PRINT 射精（
	PRINTS NAME:TARGET
	PRINT ）
	BAR BASE:2,MAXBASE:2,32
	PRINTFORML ({BASE:2}/{MAXBASE:2})
ENDIF

@LIFE_BAR
PRINT 体力
A = BASE:0
SIF BASE:0 < 0
	A = 0
BAR A,MAXBASE:0,32
PRINTFORML ({A}/{MAXBASE:0})

@STAIN_INFO

REPEAT 5
	SIF COUNT == 2 && TALENT:MASTER:121 == 0 && TALENT:MASTER:122 == 0
		CONTINUE
	SIF COUNT == 3 && TALENT:MASTER:122
		CONTINUE
	PRINTS NAME:MASTER
	IF COUNT == 0
		PRINT の口：
	ELSEIF COUNT == 1
		PRINT の手：
	ELSEIF COUNT == 2
		PRINT のＰ：
	ELSEIF COUNT == 3
		PRINT のＶ：
	ELSEIF COUNT == 4
		PRINT のＡ：
	ENDIF
	SIF STAIN:MASTER:COUNT & 1
		PRINT <Ｖ>
	SIF STAIN:MASTER::COUNT & 2
		PRINT <Ｐ>
	SIF STAIN:MASTER::COUNT & 4
		PRINT <精>
	SIF STAIN:MASTER::COUNT & 8
		PRINT <Ａ>
	PRINTL 
REND

REPEAT 5
	SIF COUNT == 2 && TALENT:121 == 0 && TALENT:122 == 0
		CONTINUE
	SIF COUNT == 3 && TALENT:122
		CONTINUE
	PRINTS NAME:TARGET
	IF COUNT == 0
		PRINT の口：
	ELSEIF COUNT == 1
		PRINT の手：
	ELSEIF COUNT == 2
		PRINT のＰ：
	ELSEIF COUNT == 3
		PRINT のＶ：
	ELSEIF COUNT == 4
		PRINT のＡ：
	ENDIF
	
	SIF STAIN:COUNT & 1
		PRINT <Ｖ>
	SIF STAIN:COUNT & 2
		PRINT <Ｐ>
	SIF STAIN:COUNT & 4
		PRINT <精>
	SIF STAIN:COUNT & 8
		PRINT <Ａ>
	PRINTL 
REND

REPEAT 5
	SIF ASSI < 0
		BREAK
	SIF COUNT == 2 && TALENT:121 == 0 && TALENT:122 == 0
		CONTINUE
	SIF COUNT == 3 && TALENT:122
		CONTINUE
	PRINTS NAME:ASSI
	IF COUNT == 0
		PRINT の口：
	ELSEIF COUNT == 1
		PRINT の手：
	ELSEIF COUNT == 2
		PRINT のＰ：
	ELSEIF COUNT == 3
		PRINT のＶ：
	ELSEIF COUNT == 4
		PRINT のＡ：
	ENDIF
	
	SIF STAIN:ASSI:COUNT & 1
		PRINT <Ｖ>
	SIF STAIN:ASSI:COUNT & 2
		PRINT <Ｐ>
	SIF STAIN:ASSI:COUNT & 4
		PRINT <精>
	SIF STAIN:ASSI:COUNT & 8
		PRINT <Ａ>
	PRINTL 
REND

WAIT

@SAVEINFO

IF TIME == 0
	PUTFORM {DAY+1}日目午前 
ELSE
	PUTFORM {DAY+1}日目午後 
ENDIF

IF TARGET >= 0
	PUTFORM %NAME:TARGET%調教中(従{ABL:0}欲{ABL:1}技{ABL:2}) 
ENDIF

SIF ASSI >= 0
	PUTFORM 助手：%NAME:ASSI%
