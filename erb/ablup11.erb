@ABLUP11
DRAWLINE
IF ABL:11 >= 5
	PRINTW 既にMAXです。
	RETURN 0
ENDIF

;条件別にＯＫかダメかを記録する
I = 0
J = 0

IF ABL:11 == 0
	A = 3000
	B = 10000
	C = 10
ELSEIF ABL:11 == 1
	A = 8000
	B = 25000
	C = 25
ELSEIF ABL:11 == 2
	A = 15000
	B = 50000
	C = 40
ELSEIF ABL:11 == 3
	A = 30000
	B = 100000
	C = 80
	;一線越えない
	IF TALENT:27
		TIMES A , 2.00
		TIMES B , 2.00
		TIMES C , 2.00
	ENDIF
ELSEIF ABL:11 == 4
	A = 55000
	B = 300000
	C = 200
	;一線越えない
	IF TALENT:27
		TIMES A , 3.00
		TIMES B , 3.00
		TIMES C , 3.00
	ENDIF
ENDIF

;汚臭鈍感
IF TALENT:61
	TIMES A , 0.25
	TIMES B , 0.25
	TIMES C , 0.25
ENDIF

;汚臭敏感
IF TALENT:62
	TIMES A , 2.50
	TIMES B , 2.50
	TIMES C , 2.50
ENDIF

;中毒しやすい
IF TALENT:72
	TIMES A , 0.50
	TIMES B , 0.50
	TIMES C , 0.50
	TIMES D , 0.50
ENDIF

;倒錯的
IF TALENT:80
	TIMES A , 0.75
	TIMES B , 0.75
	TIMES C , 0.75
	TIMES D , 0.75
ENDIF

;ＬＶ２から３、ＬＶ３から４、４から５に上げるときは異常経験必要（素質：[汚臭鈍感][中毒しやすい]なら無視できる）
IF ABL:11 == 2 && TALENT:61 == 0 && TALENT:72 == 0
	PRINTS EXPNAME:50
	PRINTL あり、かつ
	IF EXP:50 == 0
		;異常経験が不足
		I |= 2
		J |= 2
	ENDIF
ELSEIF ABL:11 == 3 && TALENT:61 == 0 && TALENT:72 == 0
	PRINTS EXPNAME:50
	PRINTL 2以上、かつ
	IF EXP:50 < 2
		;異常経験が不足
		I |= 2
		J |= 2
	ENDIF
ELSEIF ABL:11 == 4 && TALENT:61 == 0 && TALENT:72 == 0
	PRINTS EXPNAME:50
	PRINTL 3以上、かつ
	IF EXP:50 < 3
		;異常経験が不足
		I |= 2
		J |= 2
	ENDIF
ENDIF

;奉仕精神が必要
PRINTS ABLNAME:6
PRINTV ABL:11 + 1
PRINTL LV以上、かつ
IF ABL:6 < ABL:11 + 1
	I |= 4
	J |= 4
ENDIF

;欲情の珠で上げる
SIF JUEL:5 < A
	I |= 1
;充実の珠で上げる
SIF JUEL:6 < B
	I |= 1
;精液経験が必要
SIF EXP:20 < C
	I |= 2

PRINT [0] - 
PRINTS PALAMNAME:5
PRINT の珠×
PRINTV A
PRINT 、
PRINTS PALAMNAME:6
PRINT の珠×
PRINTV B
PRINT 、
PRINTS EXPNAME:20
PRINTV C
PRINT 以上
PRINT ……

IF I == 0
	PRINT ＯＫ
ELSE
	SIF I & 1
		PRINT 珠不足 
	SIF I & 2
		PRINT 経験不足 
	SIF I & 4
		PRINT 能力不足 
ENDIF
PRINTL 



;欲情の珠で上げる
SIF JUEL:5 < A*3
	J |= 1
;充実の珠で上げる
SIF JUEL:6 < B*3
	J |= 1
;精液経験が必要
SIF EXP:20 < C/2
	J |= 2

PRINT [1] - 
PRINTS PALAMNAME:5
PRINT の珠×
PRINTV A*3
PRINT 、
PRINTS PALAMNAME:6
PRINT の珠×
PRINTV B*3
PRINT 、
PRINTS EXPNAME:20
PRINTV C/2
PRINT 以上
PRINT ……

IF J == 0
	PRINT ＯＫ
ELSE
	SIF J & 1
		PRINT 珠不足 
	SIF J & 2
		PRINT 経験不足 
	SIF J & 4
		PRINT 能力不足 
ENDIF
PRINTL 


PRINTL [100] - やめる


INPUT
IF (RESULT < 0 || RESULT > 1) && RESULT != 100
	RESTART
ELSEIF I != 0 && RESULT == 0
	PRINTL 条件を満たしていません。
	RESTART
ELSEIF J != 0 && RESULT == 1
	PRINTL 条件を満たしていません。
	RESTART
ELSEIF RESULT == 100
	RETURN 0
ENDIF

ABL:11 += 1

IF RESULT == 0
	JUEL:5 -= A
	JUEL:6 -= B
ELSEIF RESULT == 1
	JUEL:5 -= A*3
	JUEL:6 -= B*3
ENDIF

